<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>


<AnchorPane id="body" prefHeight="834.0" prefWidth="1194.0" stylesheets="@styles.css"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.pdf_generated.Controller">
    <AnchorPane layoutX="297.0" layoutY="273.0" prefHeight="224.0" prefWidth="600.0"
        id="terminate-inventory">
        <children>
            <ImageView
                fx:id="imageView"
                fitHeight="50.0"
                fitWidth="50.0"
                layoutX="275.0"
                layoutY="20.0"
                preserveRatio="true"
            >
                <image>
                    <Image
                        url="@../../resources/icons/pdf-done.png"
                        backgroundLoading="true"
                    />
                </image>
            </ImageView>
        </children>
        <children>
            <VBox layoutX="94" layoutY="85">
                <children>
                    <Label text="État des lieux terminé !" prefWidth="411"
                        textAlignment="CENTER"
                        alignment="CENTER" />
                </children>
                <children>
                    <Label
                        text="Le lisible de l’état des lieux a été ajouté aux téléchargements."
                        textAlignment="CENTER" alignment="CENTER" styleClass="label-small" prefWidth="411"/>
                </children>
            </VBox>
        </children>
        <children>
            <VBox layoutX="150" layoutY="166" prefWidth="300" spacing="5">
                <children>
                    <!-- Button take max vbox width -->
                    <Button
                        mnemonicParsing="false"
                        onAction="#goBackToMainMenu" text="Revenir au menu"
                        minHeight="38"
                        prefWidth="300"
                        fx:id="menuButton"
                        />
                </children>
            </VBox>
        </children>
    </AnchorPane>
</AnchorPane>